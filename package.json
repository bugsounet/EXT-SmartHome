{
  "name": "EXT-Website",
  "version": "1.0.0",
  "rev": "240511",
  "description": "GA EXT-Website Module",
  "main": "EXT-Website.js",
  "scripts": {
    "preinstall": "installer/preinstall.sh -d 'nginx certbot python3-certbot-nginx'",
    "postinstall": "installer/postinstall.sh -b -r",
    "update": "installer/update.sh",
    "rebuild": "installer/rebuild.sh",
    "reset": "git reset --hard",
    "clean": "rm -rf node_modules package-lock.json",
    "smarthome": "sudo node installer/setup && npm run certbot",
    "certbot": "sudo installer/certbot.sh",
    "project": "installer/check_ProjectID.sh",
    "migrate": "node installer/migrate.js",
    "lint": "eslint *.js installer/*.js components/*.js --config eslint.config.js",
    "lint:fix": "eslint *.js installer/*.js components/*.js --config eslint.config.js --fix",
    "test": "npm run lint",
    "test:minify": "cd installer && node minify.js"
  },
  "keywords": [
    "MMM-GoogleAssistant",
    "magic mirror",
    "module",
    "sensor",
    "motion detection",
    "interface",
    "website",
    "EXT",
    "GoogleAssistant"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@electron/rebuild": "^3.6.0",
    "actions-on-google": "^3.0.0",
    "body-parser": "^1.20.2",
    "esbuild": "^0.20.2",
    "express-session": "^1.18.0",
    "glob": "^10.3.12",
    "google-auth-library": "^9.9.0",
    "googleapis": "^137.1.0",
    "jsoneditor": "^10.0.3",
    "lodash": "^4.17.21",
    "node-pty": "^1.0.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.3.1",
    "prompt": "^1.3.0",
    "readline": "^1.3.0",
    "semver": "^7.6.2",
    "stream": "^0.0.2",
    "systeminformation": "^5.22.8",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@stylistic/eslint-plugin": "^1.8.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bugsounet/EXT-Website"
  },
  "author": "bugsounet",
  "license": "MIT",
  "bugs": {
    "url": "https://forum.bugsounet.fr/"
  },
  "homepage": "https://wiki.bugsounet.fr/en/EXT-Website"
}
